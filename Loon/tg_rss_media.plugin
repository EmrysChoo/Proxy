#!name=TG RSS 媒体内嵌
#!desc=在RSS阅读器中直接显示tg.i-c-a.su图片和视频
#!openUrl=https://github.com/EmrysChoo/Proxy
#!author=EmrysChoo
#!system=ios

[Rewrite]
# 仅匹配RSS/XML响应
^https?:\/\/.*\.(xml|rss) script-response-body tg_rss_media_embed

[Script]
tg_rss_media_embed = type=http-response,requires-body=1,max-size=0,timeout=60,script-path=https://github.com/EmrysChoo/Proxy/raw/refs/heads/main/Loon/tg_rss_media_embed.js

[MITM]
hostname = tg.i-c-a.su
